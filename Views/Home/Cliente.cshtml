@using TEste.Models;
@model Cliente;
<h3>Cliente</h3>



<form asp-controller="Home" asp-action="Cliente">
    <div asp-validation-summary="ModelOnly"></div>


    <div class="form-group">
        Nome:
        <label for="nome"></label>
        <input class="form-control" asp-for="nome" /><br />
        <span asp-validation-for="nome"></span>
    </div>

    <div class="form-group">
        Email:
        <label for="email"></label>
        <input class="form-control" asp-for="email" /><br />
        <span asp-validation-for="email"></span>
    </div>

    <div class="form-group">
        Dt. Nasc.:
        <label for="dataNascimento"></label>
        <input class="form-control" asp-for="dataNascimento" /><br />
        <span asp-validation-for="dataNascimento"></span>
    </div>


    <button type="submit">Salvar</button>
</form>
